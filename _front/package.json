		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.14",
		"publint": "^0.2.0",
		"svelte": "^5.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.3.2",
		"vite": "^5.0.11"
	},
	"dependencies": {
		"axios": "^1.7.7",
